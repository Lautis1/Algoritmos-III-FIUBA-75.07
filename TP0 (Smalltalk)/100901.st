Object subclass: #Alumno	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'tp0-100901'!!Alumno methodsFor: 'Metodos' stamp: 'LautiStroia 8/28/2018 13:07'!establecerNombre: unNombre	"Agrega unNombre al alumno"	|nombre|	nombre:= String new.	nombre := unNombre.! !Object subclass: #Curso	instanceVariableNames: 'diccAlumnoPadron diccPadronNotas'	classVariableNames: ''	poolDictionaries: ''	category: 'tp0-100901'!!Curso methodsFor: 'Metodos' stamp: 'LautiStroia 8/28/2018 23:14'!promedioDe: unPadron	"Devuelve el promedio total del alumno 'unPadron'"		|conjNotas cantidadNotas sumatoria promedio indice|	conjNotas := diccPadronNotas  at: unPadron.	cantidadNotas := conjNotas size.	sumatoria := 0.	indice := 1.	[indice <= cantidadNotas] whileTrue: [ 		 sumatoria := sumatoria + (conjNotas at: indice).		 indice := indice + 1.	].	promedio := sumatoria/cantidadNotas.	^promedio							! !!Curso methodsFor: 'Metodos' stamp: 'LautiStroia 8/28/2018 23:13'!inscribirAlumno: nombreAlumno conNumeroDePadron: unNumero	"Agrega al curso un alumno con dado numero de padron"	diccAlumnoPadron at: nombreAlumno put: unNumero.	! !!Curso methodsFor: 'Metodos' stamp: 'LautiStroia 8/29/2018 00:16'!calificarA: padron conNota: unaNota	"Agrega al alumno con dado padron, la nota dada"	"si en la posicion 'padron' del diccionario no hay nada, creo un array nuevo.	si hay algo, agrego la nota al array."	|notas|		diccPadronNotas at: padron ifPresent: [ 		(diccPadronNotas at: padron) addLast: unaNota.		].	diccPadronNotas at: padron ifAbsent: [ 		notas := OrderedCollection new.		notas addFirst: unaNota.		diccPadronNotas  at: padron put: notas.		].		 					! !!Curso methodsFor: 'Metodos' stamp: 'LautiStroia 8/28/2018 14:03'!aprobo: unPadron	"Indica si el alumno con padron 'unPadron' tiene promedio mayor o igual que 4"	|promedio|	promedio := self promedioDe: unPadron.	^(promedio >= 4)		! !!Curso methodsFor: 'Metodos' stamp: 'LautiStroia 8/28/2018 23:13'!establecerNombre: unNombre	"Establece un nombre al curso"	|nombre|	nombre:= String new.	nombre:= unNombre.	! !!Curso methodsFor: 'Metodos' stamp: 'LautiStroia 8/28/2018 23:13'!initialize	"Crea una instancia de la clase"	diccAlumnoPadron  := Dictionary new.	diccPadronNotas  := Dictionary new.		! !